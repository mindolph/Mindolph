<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <VBox prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <ToolBar prefHeight="40.0" prefWidth="200.0">
            <Button fx:id="btnClear" mnemonicParsing="false" text="Clear" onAction="#onBtnClear"/>
            <Button fx:id="btnOpen" mnemonicParsing="false" text="Open"/>
            <CheckBox fx:id="checkBox" mnemonicParsing="false" text="Open In System"/>
        </ToolBar>
        <TextField fx:id="textField" VBox.vgrow="NEVER"/>
    </VBox>
</AnchorPane>
